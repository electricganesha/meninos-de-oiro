<div class="container-fluid" style="width:80vw; margin-left:8vw;" ng-show="show == 'structure'" ng-controller="StructureCtrl" ng-init="alert=false; success=false;">
  <div ng-repeat="item in structuralInfo">
    <div ng-if="item.textId == 'noticia_principal'">
    <h3 class="form-signin-heading" style="text-align:center; margin-bottom:50px; border-bottom: 1px solid #000; padding-bottom:15px;"> Cabeçalho </h3>
    <p id="cabecalhoTitle" contenteditable="true" ng-model="cabecalhoTitle" contentLength="50">{{item.title}} </p>
    <p style="border-top:1px solid black; width:75px;" ng-init="cabecalhoTitle.originalLength = 50"><b>{{item.title.length}}</b> / <b>{{cabecalhoTitle.originalLength}}</b></p>
    <p id="cabecalhoTexto" contenteditable="true" ng-model="cabecalhoTexto" contentLength="1000">{{item.text}}</p>
    <p style="border-top:1px solid black; width:75px;"ng-init="cabecalhoTexto.originalLength = 1000"><b>{{item.text.length}}</b> / <b>{{cabecalhoTexto.originalLength}}</b></p>
    </div>
    <div ng-if="item.textId == 'noticia_principal'">
      <h3 class="form-signin-heading" style="text-align:center; margin-bottom:50px; border-bottom: 1px solid #000; padding-bottom:15px;"> Doações </h3>
        <p id="doacoesTitle1" contenteditable="true" ng-model="doacoesTitle1" contentLength="40">{{item.tituloDoacao1}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTitle1.originalLength = 40"><b>{{item.tituloDoacao1.length}}</b> / <b>{{doacaoTitle1.originalLength}}</b></p>
        <p id="doacoesText1" contenteditable="true" ng-model="doacoesText1" contentLength="1000">{{item.textoDoacao1}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoText1.originalLength = 1000"><b>{{item.textoDoacao1.length}}</b> / <b>{{doacaoText1.originalLength}}</b></p>
        <p id="doacoesTitle2" contenteditable="true" ng-model="doacoesTitle2" contentLength="40">{{item.tituloDoacao2}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTitle2.originalLength = 40"><b>{{item.tituloDoacao2.length}}</b> / <b>{{doacaoTitle2.originalLength}}</b></p>
        <p id="doacoesText2" contenteditable="true" ng-model="doacoesText2" contentLength="1000">{{item.textoDoacao2}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoText2.originalLength = 1000"><b>{{item.textoDoacao2.length}}</b> / <b>{{doacaoText2.originalLength}}</b></p>
        <p id="doacoesTitle3" contenteditable="true" ng-model="doacoesTitle3" contentLength="40">{{item.tituloDoacao3}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTitle3.originalLength = 40"><b>{{item.tituloDoacao3.length}}</b> / <b>{{doacaoTitle3.originalLength}}</b></p>
        <p id="doacoesText3" contenteditable="true" ng-model="doacoesText3" contentLength="1000">{{item.textoDoacao3}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoText3.originalLength = 1000"><b>{{item.textoDoacao3.length}}</b> / <b>{{doacaoText3.originalLength}}</b></p>
        <p id="doacoesTitle4" contenteditable="true" ng-model="doacoesTitle4" contentLength="40">{{item.tituloDoacao4}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTitle4.originalLength = 40"><b>{{item.tituloDoacao4.length}}</b> / <b>{{doacaoTitle4.originalLength}}</b></p>
        <p id="doacoesText4" contenteditable="true" ng-model="doacoesText4" contentLength="1000">{{item.textoDoacao4}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoText4.originalLength = 1000"><b>{{item.textoDoacao4.length}}</b> / <b>{{doacaoText4.originalLength}}</b></p>
    </div>
    <div ng-if="item.textId == 'noticia_principal'" style="margin-bottom:100px;">
      <h3 class="form-signin-heading" style="text-align:center; margin-bottom:100px; border-bottom: 1px solid #000; padding-bottom:15px; "> Métodos de Doação </h3>
        <h4> Texto PayPal</h4>
        <p id="textoPaypal" contenteditable="true" ng-model="doacoesTitle" contentLength="1000">{{item.textoPaypal}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTextoPaypal.originalLength = 1000"><b>{{item.textoPaypal.length}}</b> / <b>{{doacaoTextoPaypal.originalLength}}</b></p>
        <h4> Texto Chamada</h4>
        <p id="textoChamada" contenteditable="true" ng-model="doacoesText" contentLength="1000">{{item.textoChamada}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTextoChamada.originalLength = 1000"><b>{{item.textoChamada.length}}</b> / <b>{{doacaoTextoChamada.originalLength}}</b></p>
