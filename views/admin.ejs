<div class="container-fluid" style="width:90vw; margin-left:5vw;" ng-show="show == 'stories'" ng-controller="StoryCtrl">
  <div class="procedure-details" ng-show="showDetailsStories" style="position:absolute; left:22.5vw; background-color:white; width:50vw; padding:40px; border-radius:25px; z-index:2;">
    <!-- LOGIN FORM -->
    <button type="button" ng-click="showDetailsStories = ! showDetailsStories" class="btn btn-xs btn-danger" style="float:right; border-radius:25px;">
    <i class="glyphicon glyphicon-remove-sign"></i>
    </button>
    <form ng-submit="addStory()" name="newStoryForm">
        <h2 style="margin-bottom:20px;"><u>Nova História</u></h2>
        <div class="form-group">
            <label>Título:</label>
            <input type="text" class="form-control" maxlength="40" name="tituloStory" ng-model="tituloStory" placeholder="Por favor introduza o titulo da história.">
        </div>
        <div class="form-group">
            <label>Corpo:</label>
            <textarea class="form-control" rows="5" maxlength="600" id="comment" name="corpoStory" ng-model="corpoStory" placeholder="Por favor introduza o corpo da história."></textarea>
        </div>
        <div class="form-group">
            <label>Link Para Vídeo:</label>
            <input type="text" class="form-control" name="linkStory" ng-model="linkStory" placeholder="Por favor introduza o link do vídeo que quer associar a esta história.">
        </div>
        <div>
        <button type="submit" class="btn btn-sm btn-success" style="float:right; margin-top:10px;" ng-click="showDetailsStories = ! showDetailsStories">
        <i class="glyphicon glyphicon-ok"></i> &nbsp;  &nbsp; &nbsp;Inserir História
        </button>
        </div>
    </form>
 </div>
    <table st-table="displayCollectionStories" class="table table-striped" st-safe-src="rowCollectionStories" st-watch-changes="true">
	<thead>
	<tr>
		<th style="width:200px;">Título</th>
		<th>Corpo</th>
		<th>Link para Vídeo</th>
    <th style="width:80px;">
      <button type="button" ng-click="showDetailsStories = ! showDetailsStories" class="btn btn-sm btn-success" style="float:left; margin-left:-5px;">
      <i class="glyphicon glyphicon-plus-sign"></i> Inserir
    </button>
    </th>
	</tr>
	</thead>
	<tbody>
  	<tr ng-repeat="story in stories" ng-init="convertVideoLinksToTrust()">
      <td><div contentEditable="true" ng-model="row.titleStory" contentLength="40">{{story.title}}</div>
