          <li style="display:inline; padding-right:20px;"><img src="images/icon/browser.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'structure'" ng-mouseover="displayCat = 'Estrutura'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/image.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'stories'" ng-mouseover="displayCat = 'Histórias'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/user.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'partners'" ng-mouseover="displayCat = 'Parceiros'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/team.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'team'" ng-mouseover="displayCat = 'Equipa'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/folder.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'projects'" ng-mouseover="displayCat = 'Projectos'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/stats.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'stats'" ng-mouseover="displayCat = 'Estatísticas'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/coins.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'finance'" ng-mouseover="displayCat = 'Finanças'"></li>
          <li style="display:inline; padding-right:20px;"><img src="images/icon/members.png" style="width:54px; height:54px; cursor:pointer;" ng-click="show = 'members'" ng-mouseover="displayCat = 'Membros'"></li>
