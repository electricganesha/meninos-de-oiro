      <td> <div ng-model="row.descricaoStat">{{member.name}}</div></td>
      <td> <div ng-model="row.numeroStat">{{member.email}}</div></div></td>
      <td> <div ng-model="row.numeroStat">{{member.telephone}}</div></div></td>
      <td> <div ng-model="row.numeroStat">{{member.address}}</div></div></td>
      <td> <div ng-model="row.numeroStat">{{member.birthDate | date :'dd-M-yyyy'}}</div></div></td>
      <td> <div ng-model="row.numeroStat">{{member.subscribedToNewsLetter | boolText}}</div></div></td>
      <td style="width=50px;">
        <button style="margin-left:2.5%; margin-top:2.5%;"type="button" ng-click="removeMember(row,member)" class="btn btn-sm btn-danger">
        <i class="glyphicon glyphicon-trash">
        </i>
        </button>
