        <p id="textoTransferencia" contenteditable="true" ng-model="doacoesText" contentLength="1000">{{item.textoTransferencia}}</p>
        <p style="border-top:1px solid black; width:75px;" ng-init="doacaoTextoTransferencia.originalLength = 1000"><b>{{item.textoTransferencia.length}}</b> / <b>{{doacaoTextoTransferencia.originalLength}}</b></p>
        <h4> Texto Doação</h4>
        <p id="textoDoacao" contenteditable="true" ng-model="doacoesText" contentLength="1000">{{item.textoDoacao}}</p>
