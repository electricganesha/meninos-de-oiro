<div class="container-fluid" style="width:60vw; margin-left:20vw;" ng-show="show == 'categories'" ng-controller="CategoryCtrl">
  <div class="procedure-details" ng-show="showDetailsCategory" style="position:absolute; left:22.5vw; background-color:white; width:50vw; padding:40px; border-radius:25px; z-index:2;">
