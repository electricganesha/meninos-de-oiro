          </div>
    <div class="row">
        <!-- LOCAL INFORMATION -->
        </div>

    </div>
<div class="container-fluid" style="margin:15px;" ng-init="show = 'news';" ng-show="show == 'news'">
  <div class="col-sm-6" style="width:90vw; margin-left:3.5vw;">
  <div class="row" ng-controller="TableCtrl">
    <div class="procedure-details" ng-show="showDetails" style="position:absolute; left:22.5vw; background-color:white; width:50vw; padding:40px; border-radius:25px; z-index:2;">
      <!-- LOGIN FORM -->
      <button type="button" ng-click="showDetails = ! showDetails" class="btn btn-xs btn-danger" style="float:right; border-radius:25px;">
      <i class="glyphicon glyphicon-remove-sign"></i>
      </button>
      <form ng-submit="addNews()" name="newNewsForm">
          <h2 style="margin-bottom:20px;"><u>Nova Notícia</u></h2>
          <div class="form-group">
              <label>Categoria:</label>
                <select class="form-control" name="categoriaSelect" ng-model="categoria" ng-options="category for category in getStructureCategories()">
                </select>
          </div>
          <div class="form-group">
              <label>Título:</label>
              <input type="text" maxlength="60" class="form-control" name="titulo" ng-model="titulo" placeholder="Por favor introduza o título da notícia.">
          </div>
          <div class="form-group">
              <label>URL / Link:</label>
              <input type="text" class="form-control" name="link" ng-model="link" placeholder="Por favor introduza o URL/Link da notícia.">
          </div>
          <div class="form-group">
              <label>Rubrica:</label>
              <input type="text" maxlength="100" class="form-control" name="rubrica" ng-model="rubrica" placeholder="Por favor introduza a rubrica notícia.">
          </div>
          <div class="form-group" style="display:inline-block;">
            <label>Imagem:</label>
            <br>
            <label for="files" class="btn btn-sm btn-success" style="margin-left:5px;">Escolha uma imagem do seu computador</label>&nbsp; ou &nbsp; <input type="text" class="form-control" name="imagem" ng-model="imagem" placeholder="Introduza o link da imagem." style="display:inline-block; width:15vw;">
            <input id="files" onchange="angular.element(this).scope().fileNameChanged(this)" ng-model="imagem" type="file" class="form-control" name="uploadImageFile" placeholder="Por favor escolha uma imagem do seu computador." style="visibility:hidden;">
          </div>
          <div class="form-group">
              <label>Texto:</label>
              <input type="text" maxlength="1000" class="form-control" name="texto" ng-model="texto" placeholder="Por favor introduza o texto de corpo da notícia.">
          </div>
          <div>
          <button type="submit" class="btn btn-sm btn-success" style="float:right; margin-top:10px;" ng-click="showDetails = ! showDetails">
          <i class="glyphicon glyphicon-ok"></i> &nbsp;  &nbsp; &nbsp;Inserir Notícia
          </button>
          </div>
      </form>
   </div>
<table st-table="rowCollection" class="table table-hover" style="height:70vh; width:100%;" st-safe-src="collection" st-watch-changes="true">
  <thead>
  <tr>
    <th width="125px;" style="border-bottom:1px solid;">Categoria</th>
    <th style="border-bottom:1px solid;">Titulo</th>
    <th style="border-bottom:1px solid;">Link</th>
    <th style="border-bottom:1px solid;">Titulo Extendido</th>
    <th width="195px;" style="border-bottom:1px solid;">Imagem</th>
    <th width="600px;" style="border-bottom:1px solid;">Texto</th>
    <th width="90px;" style="border-bottom:1px solid;">Data</th>
    <th width="60px;" style="border-bottom:1px solid;">Likes</th>
    <th width="80px;" style="border-bottom:1px solid;">
      <button type="button" ng-click="showDetails = ! showDetails" class="btn btn-sm btn-success" style="float:left; margin-left:-5px;">
      <i class="glyphicon glyphicon-plus-sign"></i> Inserir
      </button>
    </th>
  </tr>
  </thead>
  <tbody>
  <tr ng-repeat="newsPost in news | orderBy:'-date'">
    <td style="width:125px;" >
      <select class="form-control" ng-init="optionCategory = newsPost.category" name="categoriaSelect" ng-model="optionCategory" ng-options="category for category in getStructureCategories()" ng-change="changeCategory(optionCategory,newsPost)">
      </select>
    </td>
    <td><div contentEditable="true" ng-model="row.title" contentLength="50">{{newsPost.title}}</div><p style="border-top:1px solid black; width:75px; margin-top:20px;" ng-init="newsPostTitle.originalLength = 50"><b>{{newsPost.title.length}}</b> / <b>{{newsPostTitle.originalLength}}</b></p></td>
    <td><div contentEditable="true" ng-model="row.link">{{newsPost.link}}</div></td>
    <td><div contentEditable="true" ng-model="row.body" contentLength="100">{{newsPost.body}}</div><p style="border-top:1px solid black; width:75px; margin-top:20px;" ng-init="newsPostBody.originalLength = 100"><b>{{newsPost.body.length}}</b> / <b>{{newsPostBody.originalLength}}</b></p></td>
    <td>
      <div style=" position: relative;  width: 180px; height: 100px; overflow: hidden; border:1px solid black;" >
        <img src="{{newsPost.imageLink}}" style="cursor: pointer; position: absolute; left: 50%; top: 50%; height: 100%; width: auto; -webkit-transform: translate(-50%,-50%); -ms-transform: translate(-50%,-50%); transform: translate(-50%,-50%);">
      <div>
      </div>
      </div>
          <input id="file-input" type="file" style="display:block" ng-model="newsImage" data-file="param.file"/>
      </div>
      <div contentEditable="true" style=" border:1px solid black;position:relative; width:180px; color:black; text-align:left; margin-top:10px;" ng-model="row.imageLink"> {{newsPost.imageLink}} </div>
      </div>
    </td>
    <td><div contentEditable="true" ng-model="row.text" contentLength="1000">{{newsPost.text}}</div><p style="border-top:1px solid black; width:75px; margin-top:20px;" ng-init="newsPostText.originalLength = 1000"><b>{{newsPost.text.length}}</b> / <b>{{newsPostText.originalLength}}</b></p></td>
    <td><div ng-model="row.date" style=" width:80px; font-size:9px;">{{newsPost.date | date :'dd-M-yyyy H:mm'}}</td>
    <td><div ng-model="row.upvotes" style="font-size:9px;">{{newsPost.upvotes}}</td>
    <td style="width:50px;">
<button style="margin-left:25%;"type="button" ng-click="removeItem(row,newsPost)" class="btn btn-sm btn-danger">
<i class="glyphicon glyphicon-trash">
</i>
</button>
</td>
  </tr>
  </tbody>
  <tfoot>
    <tr>
				<td colspan="5" class="text-center">
					<div st-pagination="" st-items-by-page="itemsByPage" st-displayed-pages="7"></div>
				</td>
			</tr>
  </tfoot>
</table>
</div>
</div>
</div>
